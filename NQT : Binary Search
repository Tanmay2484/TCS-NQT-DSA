#include <bits/stdc++.h>
using namespace std;
int main() {
   int arr[5]={1,3,4,5,6};
   int n=4,i=0,key=1,check=0;
   while(i<n){
       int mid=i+(n-i)/2;
       if(arr[mid]==key){
           cout<<"find it";
           check=1;
           break;
       }
       if(arr[mid]>key){
          n=mid-1;
          
       }
       if(arr[mid]<key){
            i=mid+1;
       }
      
   }
   if(check==0) cout<<"not found";
}
