#include<bits/stdc++.h>
using namespace std;
int main() {
   string s="pleasesaveme";
    int k=sqrt(s.size()),l=k+1,count=0;
    for(int i=0;i<s.size();i++){
        for(int j=0;j<s.size();j+=l){
               if(i+j<=s.size()-1){
                   cout<<s[i+j];   
               }
        }
         if(count==k) break;  
        cout<<" ";
        count++;
    }
}
