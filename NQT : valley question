int countingValleys(int steps, string path) {
         int count=0,temp=0;
    for(int i=0;i<steps;i++){
        if(path[i]=='U'){
            temp++;
        }
        else{
           
             if(temp==0)
                count++; 
             temp--;
            
        }
           
    }
    return count;
}
