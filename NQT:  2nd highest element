#include <bits/stdc++.h>
using namespace std;
int main() {
    int arr[5]={4,2,3,10,5};
    int temp,maxi=0;
    for(int i=0;i<5;i++){
        if(maxi<arr[i])
        {
           temp=maxi;
            maxi=arr[i];       
        }
        else if(arr[i]>temp)
            temp=arr[i];
    }
    cout<<temp;
}
