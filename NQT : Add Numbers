#include <bits/stdc++.h>
using namespace std;
int sumofdigits(int n){
    int sum=0;
    while(n>0){
        sum+=n%10;
        n/=10;
    }
    return sum;
}
int main() {
    int n,r,sum=0,ans=0,temp=0;
    cin>>n>>r;
    if(r==0){
        cout<<temp;
    }
   sum=sumofdigits(n)*r;
    while(sum>=10){
        sum=sumofdigits(sum);
     }
     cout<<sum;
     return 0;
    
}
